{
  "name": "git-version-webpack-plugin",
  "version": "4.2.0",
  "description": "Let webpack generate git version for you",
  "main": "src/index.js",
  "files": [
    "src/"
  ],
  "scripts": {
    "commit": "git-cz",
    "update-snapshots": "ava -u test/*.test.js",
    "declarations": "node_modules/.bin/tsc --declaration --allowJs src/index.js",
    "test": "ava test/*.test.js",
    "prettify:fix": "prettier --write src/*.js test/*.js",
    "prettify": "prettier --check src/*.js test/*.js",
    "lint:fix": "eslint src/*.js test/*.js --fix",
    "lint": "eslint src/*.js test/*.js",
    "coverage": "nyc npm test",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkhhuse/git-version-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "html-webpack-plugin",
    "webapp",
    "pwa",
    "manifest",
    "favicon",
    "icon",
    "png",
    "svg"
  ],
  "author": "jkhhuse <jkhhuse@126.com> (https://github.com/jkhhuse)",
  "contributors": [
    "jkhhuse <jkhhuse@126.com> (https://github.com/jkhhuse)"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jkhhuse/git-version-webpack-plugin",
  "devDependencies": {
    "ava": "^2.4.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "diffable-html": "4.0.0",
    "eslint": "^6.8.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.3.0",
    "image-size": "0.8.3",
    "nyc": "^15.0.0",
    "prettier": "1.19.1",
    "standard-version": "8.0.2",
    "typescript": "3.7.4",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/favicons": "5.5.0",
    "cache-loader": "^4.1.0",
    "camelcase": "^5.3.1",
    "favicons": "^6.2.0",
    "find-cache-dir": "^3.2.0",
    "find-root": "^1.1.0",
    "loader-utils": "^1.2.3",
    "parse-author": "^2.0.0",
    "parse5": "^5.1.0",
    "tapable": "^1.1.3"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "optionalDependencies": {
    "html-webpack-plugin": ">=4.0.0 || ^4.0.0-beta.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8.9.4"
  }
}
